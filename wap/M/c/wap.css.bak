html {
	background: #fff;
	color: #333;
	font: 15px/1 "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "WenQuanYi Micro Hei", "Helvetica Neue", Arial, sans-serif;
}

body,
h1,
h2,
h3,
p,
ol,
ul,
li,
dl,
dd,
blockquote,
fieldset,
button {
	border: 0;
	margin: 0;
	padding: 0;
	font-size: 1em
}

header,
footer,
main,
div,
p,
ul {
	box-sizing: border-box
}

ul,
ol {
	list-style: none
}

a {
	text-decoration: none
}

a img {
	vertical-align: middle
}

table {
	border-collapse: collapse;
	border-spacing: 0
}

div,
p,
h2,
li {
	padding: 0.5em;
}

a img {
	margin-right: 0.5em;
}
.right {
float: right;
padding:-0.5em;
}
