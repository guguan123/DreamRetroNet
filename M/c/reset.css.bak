@charset "utf-8";

html {
	background: #f5f5f5;
	color: #666;
	font: 15px/1 "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "WenQuanYi Micro Hei", "Helvetica Neue", Arial, sans-serif;
}

img {
-webkit-filter: grayscale(1);
filter:gray;
filter: grayscale(1);
}

body {
	-webkit-tap-highlight-color: transparent;
}

body,
h1,
h2,
h3,
p,
ol,
ul,
li,
dl,
dd,
blockquote,
fieldset,
button {
	border: 0;
	margin: 0;
	padding: 0;
	font-size: 1rem;
}

ul,
ol {
	list-style: none;
}

a {
	text-decoration: none;
}

a img {
	vertical-align: middle;
}

input,
textarea,
select {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	width: 100%;
}

input[type="submit"] {
	cursor: pointer;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
}

.box {
	background: #fff;
	/*padding: 1rem;*/
}

.game h2 {
	font-size: 1.5rem;
	margin-bottom: 1rem;
	border-bottom: dashed 1px #ddd;
	padding-bottom: 1rem;
}

.game>.game_info {
	display: grid;
	grid-gap: 1rem 0.5rem;
	margin-bottom: 1rem;
}

.game >p {
	margin-bottom: 1rem;
	padding: 1rem;
	line-height: 1.5;
	background: #f5f5f5;
}

.screens {
	display: grid;
	grid-gap: 1rem;
	margin-bottom: 1rem;
}

.screens img {
	max-width: 100%;
}

a[href^="/package"],
a[href^="/vmrp"]{
	display: block;
	padding: 1rem;
	background: #69c;
	color: #fff;
	margin: 0 auto 1rem auto;
}

.download p {
	line-height: 2;
	padding: 1rem;
	border-top: dashed 1px #ccc;
	color:#999;
}


